{
    "name": "@revertdotdev/js",
    "version": "1.0.0",
    "description": "Client side javascript sdk",
    "scripts": {
        "test": "npm test",
        "build": "babel src -d lib && browserify lib/index.js -o lib/bundle.js",
        "build-dev": "babel src --out-file lib/index-dev.js && browserify lib/index-dev.js -o lib/bundle-dev.js",
        "bundle": "browserify lib/index.js -o lib/bundle.js",
        "pre-deploy": "javascript-obfuscator lib/bundle.js --output build/revert.js",
        "pre-deploy-dev": "javascript-obfuscator lib/bundle-dev.js --output build/revert-dev.js",
        "deploy": "npm run build && npm run pre-deploy",
        "deploy-dev": "npm run build-dev && npm run pre-deploy-dev"
    },
    "author": "Jatin Sandilya",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "browserify": "^17.0.0",
        "javascript-obfuscator": "^2.10.3"
    },
    "dependencies": {
        "@fingerprintjs/fingerprintjs": "^3.0.5",
        "loose-envify": "^1.4.0",
        "scroll-into-view-if-needed": "^2.2.26",
        "tinycolor2": "^1.4.2",
        "uuid": "^8.3.2"
    }
}
